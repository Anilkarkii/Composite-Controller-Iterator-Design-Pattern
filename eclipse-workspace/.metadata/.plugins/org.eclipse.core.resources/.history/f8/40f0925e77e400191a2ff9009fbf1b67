	@Override
	public void draw(Graphics g) 
	{
	
		//g.setColor(Color.BLACK);
		
		g.drawOval(X1,Y1,40, 40);
		
	}
	
	@Override
	public void addShapeNPoint(Shape s) {
		// TODO Auto-generated method stub
		
	}
	@Override
